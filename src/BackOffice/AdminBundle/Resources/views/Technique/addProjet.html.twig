                    <td>  <label for="exampleInputEmail1">Contact </label>  </td>
                    <td>{{ form_widget(form.contact) }}</td>
                </tr>
                <tr>
                    <td>  <label for="exampleInputEmail1">Nombre de jours de préfabrication </label>  </td>
                    <td> {{ form_widget(form.nbJourPrefa) }} </td>
