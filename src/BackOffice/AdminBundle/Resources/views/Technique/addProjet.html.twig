{% extends is_granted("ROLE_ADMIN")
? "AdminBundle::layoutSUP.html.twig"
: "AdminBundle::layoutTech.html.twig"%}
{% block user_content %}
    <div class="row">
        <div class="col-lg-3 col-xs-6">
        <form action="{{ path('tech_addProjet' ) }}" {{ form_enctype(form) }} method="POST">
             <h1> Nouveau Projet </h1>
             <br>
            <table id="example2" class="table table-bordered table-hover">
                <tr>
                    <td>  <label for="exampleInputEmail1">Nature projet </label>  </td>
                    <td>{{ form_widget(form.nature) }}</td>
                    <td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td>
                    
                    <td>  <label for="exampleInputEmail1">Besoin en matériels roulant </label>  </td>
                    <td>{{ form_widget(form.besoinMaterielRoulant) }}</td>
                </tr>
                <tr>
                    <td>  <label for="exampleInputEmail1">Lieu </label>  </td>
                    <td> {{ form_widget(form.lieu) }} </td>
                    <td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td>
                    
                    <td>  <label for="exampleInputEmail1">Besoin en matière première </label>  </td>
                    <td>{{ form_widget(form.besoinMatierePremiere) }}</td>
                </tr>
                <tr>
                    <td>  <label for="exampleInputEmail1">Client </label>  </td>
                    <td> {{ form_widget(form.idClient) }} </td>
                    <td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td>
                    <td>  <label for="exampleInputEmail1">Date de mise à disposition </label>  </td>
                    <td>{{ form_widget(form.dateMiseDispo) }}</td>
                </tr>
                <tr>
                    <td>  <label for="exampleInputEmail1">Besoin en échaffaudage </label>  </td>
                    <td> {{ form_widget(form.besoinEchaffaudage) }}</td>
                    <td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td><td>&nbsp</td>
                    
                    <td>  <label for="exampleInputEmail1">Coût équipement Indiv </label>  </td>
                    <td>{{ form_widget(form.coutEPI) }}</td>
                </tr>
                <tr>
